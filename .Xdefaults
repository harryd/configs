! global settings
*foreground:   				#d3d3d3
*background:   				#000000
*cursorColor:				orange
*color0:			   		#151515
*color8:					#505450
*color1:					#bf7979
*color9:					#f4a45f
*color2:					#97b26b
*color10:					#c5f779
*color3:					#cdcda1
*color11:					#ffffaf
*color4:					#4a5463
*color12:					#7d8794
*color5:					#9c3885
*color13:					#e628ba
*color6:					#88aadd
*color14:					#99ccff
*color7:					#ffffff
!*color15:					#dedede
*color15:					#fefefe

*font:               	xft:terminus:pixelsize=12

! urxvt settings
urxvt*geometry:         	107x33
urxvt*saveLines:        	0
urxvt*scrollTtyKeypress:	true
urxvt*scrollTtyOutput: 		false
urxvt*scrollBar:        	false
urxvt*secondaryscroll:  	true
urxvt*loginShell:       	true
urxvt*internalBorder:   	0
urxvt*transparent: 			true
urxvt*termName:         	rxvt-unicode
urxvt*inheritPixmap:    	true
urxvt*shading:          	50
urxvt*tintColor:        	#555555
urxvt*print-pipe:      		"cat > $(echo urxvt.dump.`date +'%Y%M%d%H%m%S'`)"
urxvt*secondaryScroll:  	true
urxvt*mapAlert:         	true
urxvt*utmpInhibit:      	true
urxvt.perl-lib:         	/usr/lib/urxvt/perl
urxvt*perl-ext-common:  	default,matcher,searchable-scrollback
urxvt*matcher.button:   	1
urxvt*urlLauncher:      	chromium-browser
urxvt.matcher.pattern.1: 	\\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
urxvt*keysym.S-Prior:   	^[[5;2~
urxvt*keysym.S-Next:    	^[[6;2~
urxvt.urgentOnBell:  		true

! aterm shit
! aterm*loginShell:			true
aterm*transparent:			true
aterm*shading:				25
aterm*background:			Black
aterm*foreground:			White
aterm*scrollBar:			false
aterm*scrollBar_right:		true
aterm*transpscrollbar:		true
aterm*saveLines:			32767
aterm*font:					*terminus*medium*-10-*

